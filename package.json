{
    "name": "fundamental-ngx-app",
    "version": "0.0.1",
    "license": "Apache-2.0",
    "main": "dist/fundamental-ngx/index.js",
    "description": "SAP Fundamental Styles, implemented in Angular",
    "scripts": {
        "build": "ng build",
        "build-docs": "npm run compile-typedoc && npm run copy-docs && ng build --prod --base-href=\"/fundamental-ngx/\" ",
        "build-docs-netlify": "npm run compile-typedoc && npm run copy-docs && ng build --prod --base-href=\"/\" ",
        "build-deploy-library": "ng build --prod fundamental-ngx && npm run build:schematics && cp README.md dist/fundamental-ngx/README.md",
        "build-pack-library": "ng build --prod fundamental-ngx && npm run build:schematics && cd dist/fundamental-ngx && npm pack && cd ../..",
        "build-src": "gulp build-src",
        "copy-docs": "cp README.md docs/assets/README.md && cp NEW_COMPONENT.md docs/assets/NEW_COMPONENT.md",
        "deploy-docs": "angular-cli-ghpages --branch gh-pages",
        "e2e": "ng e2e",
        "hmr": "ng serve --hmr -e=hmr",
        "lint": "ng lint",
        "lint:pre-commit": "printf \"running pre-commit lint...\"  && npm run lint && printf \"done!\n\"",
        "ng": "ng",
        "release": "./scripts/publish-release.sh",
        "release:create": "create-release",
        "start": "npm run build-doc && ng serve",
        "test": "ng test fundamental-ngx --watch false --code-coverage true --browsers=ChromeHeadless",
        "active-test": "ng test fundamental-ngx --watch true --browsers=ChromeHeadless",
        "test:coveralls": "cat ./coverage/lcov.info  | ./node_modules/coveralls/bin/coveralls.js || echo -e \"Coveralls failed.\"",
        "compile-typedoc": "rm -rf docs/assets/typedoc && typedoc --out docs/assets/typedoc library/src/lib --module commonjs --mode file --exclude **/*.spec.ts --hideGenerator --excludePrivate --theme docs/fd-typedoc",
        "compile-typedoc-json": "typedoc --json docs/assets/typedoc/typedoc.json --module commonjs --mode file --exclude **/*.spec.ts --hideGenerator --excludePrivate --theme docs/fd-typedoc library/src/lib",
        "build:schematics": "./node_modules/.bin/tsc -p ./library/tsconfig.schematics.json && npm run copy:schemas && npm run copy:collection",
        "copy:schemas": "cp library/schematics/ng-add/schema.json dist/fundamental-ngx/schematics/ng-add/schema.json",
        "copy:collection": "cp library/schematics/collection.json dist/fundamental-ngx/schematics/collection.json"
    },
    "publishConfig": {
        "registry": "https://repository.hybris.com/api/npm/npm-repository/"
    },
    "dependencies": {
        "@angular/animations": "^7.2.7",
        "@angular/cdk": "^7.3.7",
        "@angular/common": "^7.2.7",
        "@angular/compiler": "^7.2.7",
        "@angular/core": "^7.2.7",
        "@angular/forms": "^7.2.7",
        "@angular/http": "^7.2.7",
        "@angular/platform-browser": "^7.2.7",
        "@angular/platform-browser-dynamic": "^7.2.7",
        "@angular/router": "^7.2.7",
        "@types/node": "^12.7.4",
        "core-js": "^2.6.9",
        "focus-trap": "^5.0.2",
        "fundamental-styles": "^0.1.0",
        "highlight.js": "^9.15.10",
        "moment": "^2.24.0",
        "ngx-highlightjs": "^3.0.3",
        "ngx-markdown": "^8.1.0",
        "npm": "^6.11.2",
        "popper.js": "^1.15.0",
        "rxjs": "^6.5.2",
        "zone.js": "^0.8.29"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.13.8",
        "@angular-devkit/build-ng-packagr": "^0.13.3",
        "@angular-devkit/core": "^7.3.9",
        "@angular/cli": "^7.3.3",
        "@angular/compiler-cli": "^7.2.7",
        "@angular/language-service": "^7.2.7",
        "@angularclass/hmr": "^2.1.3",
        "@schematics/angular": "^7.3.9",
        "@types/jasmine": "^3.4.0",
        "@types/jasminewd2": "^2.0.4",
        "angular-cli-ghpages": "^0.5.3",
        "chalk": "^2.4.2",
        "codelyzer": "^5.1.0",
        "coveralls": "^3.0.6",
        "fs-extra": "^8.1.0",
        "github-assistant": "^0.3.0",
        "jasmine-core": "^3.4.0",
        "jasmine-spec-reporter": "^4.2.1",
        "karma": "^4.3.0",
        "karma-chrome-launcher": "^3.1.0",
        "karma-cli": "^2.0.0",
        "karma-coverage-istanbul-reporter": "^2.1.0",
        "karma-jasmine": "^2.0.1",
        "karma-jasmine-html-reporter": "^1.4.2",
        "ng-packagr": "^4.7.1",
        "pre-commit": "^1.2.2",
        "prettier": "^1.16.4",
        "protractor": "^5.4.2",
        "ts-node": "^8.3.0",
        "tsickle": "^0.37.0",
        "tslib": "^1.10.0",
        "tslint": "^5.19.0",
        "typedoc": "^0.15.0",
        "typescript": "3.2.4"
    },
    "pre-commit": {
        "color": true,
        "run": [
            "lint:pre-commit"
        ],
        "silent": false
    }
}
