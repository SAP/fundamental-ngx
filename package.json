{
    "name": "fundamental-ngx-app",
    "version": "0.0.1",
    "license": "Apache-2.0",
    "main": "dist/fundamental-ngx/index.js",
    "scripts": {
        "build": "ng build",
        "build-docs": "npm run copy-docs && ng build --prod --base-href=\"/fundamental-ngx/\"",
        "build-deploy-library": "ng build --prod fundamental-ngx && cp README.md dist/fundamental-ngx/README.md",
        "build-pack-library": "ng build --prod fundamental-ngx && cd dist/fundamental-ngx && npm pack && cd ../..",
        "build-src": "gulp build-src",
        "copy-docs": "cp README.md docs/assets/README.md && cp NEW_COMPONENT.md docs/assets/NEW_COMPONENT.md",
        "deploy-docs": "angular-cli-ghpages --branch gh-pages",
        "e2e": "ng e2e",
        "hmr": "ng serve --hmr -e=hmr",
        "lint": "ng lint",
        "lint:pre-commit": "printf \"running pre-commit lint...\"  && npm run lint && printf \"done!\n\"",
        "ng": "ng",
        "release": "./scripts/publish-release.sh",
        "release:create": "create-release",
        "start": "npm run build-doc && ng serve",
        "test": "ng test fundamental-ngx --watch false --code-coverage true --browsers=ChromeHeadless",
        "test:coveralls": "cat ./coverage/lcov.info  | ./node_modules/coveralls/bin/coveralls.js || echo -e \"Coveralls failed.\""
    },
    "publishConfig": {
        "registry": "https://repository.hybris.com/api/npm/npm-repository/"
    },
    "dependencies": {
        "@angular/animations": "^7.1.4",
        "@angular/common": "^7.1.4",
        "@angular/compiler": "^7.1.4",
        "@angular/core": "^7.1.4",
        "@angular/forms": "^7.1.4",
        "@angular/http": "^7.1.4",
        "@angular/platform-browser": "^7.1.4",
        "@angular/platform-browser-dynamic": "^7.1.4",
        "@angular/router": "^7.1.4",
        "angular-highlight-js": "^2.0.1",
        "core-js": "^2.6.1",
        "fiori-fundamentals": "^1.4.3",
        "highlight.js": "^9.13.1",
        "ngx-highlightjs": "^3.0.0",
        "ngx-markdown": "^7.1.3",
        "npm": "^6.5.0",
        "raw-loader": "^1.0.0",
        "rxjs": "^6.3.3",
        "zone.js": "^0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.12.1",
        "@angular-devkit/build-ng-packagr": "^0.12.1",
        "@angular/cli": "^7.1.4",
        "@angular/compiler-cli": "^7.1.4",
        "@angular/language-service": "^7.1.4",
        "@angularclass/hmr": "^2.1.3",
        "@types/jasmine": "^2.8.8",
        "@types/jasminewd2": "^2.0.4",
        "@types/node": "^10.12.18",
        "angular-cli-ghpages": "^0.5.3",
        "codelyzer": "^4.5.0",
        "coveralls": "^3.0.2",
        "fs-extra": "^7.0.1",
        "github-assistant": "^0.2.0",
        "jasmine-core": "^3.2.1",
        "jasmine-spec-reporter": "^4.2.1",
        "karma": "^3.0.0",
        "karma-chrome-launcher": "^2.2.0",
        "karma-cli": "^1.0.1",
        "karma-coverage-istanbul-reporter": "^2.0.4",
        "karma-jasmine": "^1.1.2",
        "karma-jasmine-html-reporter": "^1.3.1",
        "ng-packagr": "^4.4.5",
        "pre-commit": "^1.2.2",
        "prettier": "^1.15.3",
        "protractor": "^5.4.2",
        "ts-node": "^7.0.1",
        "tsickle": "^0.34.0",
        "tslib": "^1.9.3",
        "tslint": "^5.12.0",
        "typescript": "3.1.1"
    },
    "pre-commit": {
        "color": true,
        "run": [
            "lint:pre-commit"
        ],
        "silent": false
    }
    
}
