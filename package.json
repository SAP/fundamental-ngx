{
    "name": "fundamental-ngx",
    "private": true,
    "license": "Apache-2.0",
    "main": "dist/libs/core/index.js",
    "description": "Fundamental Library for Angular is a themeable Fiori Angular component library.",
    "homepage": "https://sap.github.io/fundamental-ngx",
    "repository": {
        "type": "git",
        "url": "https://github.com/SAP/fundamental-ngx"
    },
    "engines": {
        "yarn": ">= 4.9.0",
        "node": ">= 20"
    },
    "packageManager": "yarn@4.9.2",
    "scripts": {
        "preinstall": "npx only-allow yarn",
        "build": "echo Building default app && nx compile",
        "build-docs-netlify": "yarn run build --configuration=production-unoptimized",
        "build:umbrella": "echo Building umbrella libraries && nx run-many --target=build --all",
        "build:umbrella:prod": "echo Building umbrella libraries PROD && yarn run build:umbrella -- --configuration=production",
        "build:prepare": "echo Building libraries and preparing for publishing && nx run-many --target=prepare --all --pack --configuration=production --skip-nx-cache",
        "test": "echo Running unit tests && nx run-many --target=test --all --parallel=10",
        "app:e2e:affected": "echo Running affected e2e tests on local && nx run docs:e2e-app:affected-local",
        "app:e2e:all": "echo Running all e2e tests on local && nx run docs:e2e-app:all-local",
        "ng": "ng",
        "hotfix-release": "node scripts/release-hotfix.js",
        "start": "nx serve",
        "test:coveralls": "cat ./coverage/libs/core/lcov.info  | ./node_modules/coveralls/bin/coveralls.js || echo -e \"Coveralls failed.\"",
        "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
        "nx": "nx",
        "postinstall": "husky install",
        "pre-commit": "lint-staged",
        "format": "prettier . --write --list-different"
    },
    "publishConfig": {
        "registry": "https://repository.hybris.com/api/npm/npm-repository/"
    },
    "dependencies": {
        "@angular-eslint/eslint-plugin-template": "20.0.0",
        "@angular-eslint/template-parser": "20.0.0",
        "@angular/animations": "20.0.2",
        "@angular/cdk": "20.0.2",
        "@angular/common": "20.0.2",
        "@angular/compiler": "20.0.2",
        "@angular/core": "20.0.2",
        "@angular/forms": "20.0.2",
        "@angular/platform-browser": "20.0.2",
        "@angular/platform-browser-dynamic": "20.0.2",
        "@angular/router": "20.0.2",
        "@fundamental-styles/cx": "0.40.1",
        "@nx/angular": "21.2.0",
        "@sap-theming/theming-base-content": "11.30.1",
        "@stackblitz/sdk": "1.9.0",
        "@swc/helpers": "0.5.11",
        "compare-versions": "6.1.0",
        "core-js": "3.26.1",
        "dayjs": "1.11.11",
        "fast-deep-equal": "3.1.3",
        "focus-trap": "7.1.0",
        "focus-visible": "5.2.1",
        "fundamental-styles": "0.40.1",
        "fuse.js": "7.0.0",
        "highlight.js": "11.7.0",
        "intl": "1.2.5",
        "intl-messageformat": "10.5.2",
        "lodash-es": "4.17.21",
        "marked": "9.0.0",
        "moment": "2.29.4",
        "ngx-markdown": "19.0.0",
        "rxjs": "7.8.2",
        "tslib": "^2.3.0",
        "yarn.lock": "^0.0.1-security",
        "zone.js": "0.15.0"
    },
    "devDependencies": {
        "@actions/core": "1.10.1",
        "@angular-devkit/build-angular": "20.0.1",
        "@angular-devkit/core": "20.0.1",
        "@angular-devkit/schematics": "20.0.1",
        "@angular/cli": "~20.0.0",
        "@angular/compiler-cli": "20.0.2",
        "@angular/language-service": "20.0.2",
        "@commitlint/cli": "18.6.1",
        "@commitlint/config-conventional": "18.6.1",
        "@jsdevtools/npm-publish": "3.0.1",
        "@nx/devkit": "21.2.0",
        "@nx/eslint": "21.2.0",
        "@nx/eslint-plugin": "21.2.0",
        "@nx/jest": "21.2.0",
        "@nx/js": "21.2.0",
        "@nx/plugin": "21.2.0",
        "@nx/workspace": "21.2.0",
        "@sap-ui/common-css": "0.40.1",
        "@schematics/angular": "20.0.1",
        "@swc-node/register": "1.9.2",
        "@swc/cli": "0.6.0",
        "@swc/core": "1.5.7",
        "@types/google.visualization": "0.0.71",
        "@types/jasmine": "5.1.3",
        "@types/jest": "29.5.14",
        "@types/lodash-es": "4.17.6",
        "@types/marked": "5.0.2",
        "@types/node": "22.10.2",
        "@wdio/allure-reporter": "7.7.3",
        "@wdio/cli": "7.16.16",
        "@wdio/jasmine-framework": "7.0.0",
        "@wdio/local-runner": "9.16.2",
        "@wdio/sauce-service": "7.16.16",
        "@wdio/spec-reporter": "7.7.3",
        "angular-eslint": "20.0.0",
        "autoprefixer": "^10.4.0",
        "axe-core": "4.5.2",
        "axe-html-reporter": "2.2.3",
        "axios": "1.6.0",
        "chromedriver": "99.0.0",
        "conventional-changelog": "4.0.0",
        "conventional-changelog-angular": "6.0.0",
        "conventional-changelog-cli": "3.0.0",
        "conventional-recommended-bump": "7.0.1",
        "coveralls": "3.1.1",
        "dependency-tree": "10.0.9",
        "eslint": "^9.28.0",
        "eslint-config-prettier": "10.1.2",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-jsdoc": "46.9.0",
        "eslint-plugin-json": "3.1.0",
        "eslint-plugin-markdown": "3.0.0",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "fast-glob": "3.3.1",
        "husky": "8.0.2",
        "jasmine-core": "4.5.0",
        "jasmine-marbles": "0.9.2",
        "jasmine-spec-reporter": "7.0.0",
        "jest": "29.7.0",
        "jest-environment-jsdom": "29.7.0",
        "jest-environment-node": "^29.4.1",
        "jest-extended": "4.0.2",
        "jest-preset-angular": "14.6.0",
        "jsonc-eslint-parser": "^2.1.0",
        "lerna": "^8.1.9",
        "lint-staged": "15.1.0",
        "ng-packagr": "20.0.0",
        "ngx-cva-test-suite": "2.0.0",
        "postcss-preset-env": "9.3.0",
        "postcss-url": "10.1.3",
        "prettier": "3.5.1",
        "prettier-plugin-organize-imports": "3.2.4",
        "semver": "7.6.3",
        "standard-version": "9.5.0",
        "through2": "4.0.2",
        "ts-jest": "29.1.0",
        "ts-node": "10.9.2",
        "tsconfig-paths": "4.1.0",
        "typedoc": "0.25.3",
        "typedoc-plugin-merge-modules": "5.1.0",
        "typescript": "5.8.2",
        "typescript-eslint": "^8.33.1",
        "wait-on": "6.0.1",
        "wdio-axe": "2.2.3",
        "wdio-chromedriver-service": "6.0.4",
        "webdriverio": "7.27.0"
    },
    "resolutions": {
        "nx": "21.2.0"
    },
    "lint-staged": {
        "*.properties": [
            "yarn nx run i18n:transform-translations"
        ],
        "*": [
            "yarn nx format:write --uncommitted"
        ],
        "*.{js,ts,html,md,json,yml}": [
            "eslint --fix --flag unstable_config_lookup_from_file"
        ]
    }
}
