<section class="tsd-panel-group tsd-member-group {{cssClasses}}">
    <h2>{{title}}</h2>
    {{#each children}}
        {{#unless hasOwnDocument}}
            {{> member}}
        {{/unless}}
    {{/each}}
</section>

<section class="tsd-panel-group tsd-member-group {{cssClasses}}">
    <h2>Inputs</h2>
    {{#each children}}
        {{#ifDecoratorsContain this 'Input'}}
            {{#unless hasOwnDocument}}
                {{> member}}
            {{/unless}}
        {{/ifDecoratorsContain}}
    {{/each}}
</section>
