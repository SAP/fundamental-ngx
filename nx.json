{
    "npmScope": "fundamental-ngx",
    "implicitDependencies": {
        "angular.json": "*",
        "package.json": "*",
        "tsconfig.json": "*",
        "nx.json": "*"
    },
    "affected": {
        "defaultBase": "main"
    },
    "tasksRunnerOptions": {
        "default": {
            "runner": "@nrwl/nx-cloud",
            "options": {
                "cacheableOperations": ["compile-typedocs", "build", "lint", "test", "e2e"],
                "parallel": 1,
                "accessToken": "YzBkOTQyNjUtN2YyNS00YWI3LThiMzQtYjFhMmU4YzJhY2FkfHJlYWQ="
            }
        }
    },
    "cli": {
        "analytics": false
    },
    "defaultProject": "docs",
    "generators": {
        "@schematics/angular:component": {
            "prefix": "fd",
            "style": "scss"
        },
        "@schematics/angular:directive": {
            "prefix": "fd"
        },
        "@nrwl/angular:application": {
            "style": "scss",
            "linter": "eslint",
            "unitTestRunner": "jest"
        },
        "@nrwl/angular:library": {
            "linter": "eslint",
            "unitTestRunner": "jest",
            "style": "scss"
        },
        "@nrwl/angular:component": {
            "style": "scss"
        },
        "@nrwl/angular": {
            "convert-tslint-to-eslint": {
                "ignoreExistingTslintConfig": false,
                "removeTSLintIfNoMoreTSLintTargets": true,
                "skipFormat": false
            }
        }
    },
    "$schema": "./node_modules/nx/schemas/nx-schema.json",
    "targetDefaults": {
        "build": {
            "dependsOn": ["^build"]
        }
    }
}
