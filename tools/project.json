{
    "sourceRoot": "tools",
    "projectType": "library",
    "targets": {
        "build": {
            "executor": "@nrwl/js:tsc",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/tools",
                "main": "libs/tools/index.ts",
                "tsConfig": "libs/tools/tsconfig.tools.json",
                "assets": [
                    {
                        "input": "./tools",
                        "glob": "**/!(*.ts)",
                        "output": "./tools"
                    },
                    {
                        "input": "./tools",
                        "glob": "**/*.d.ts",
                        "output": "./tools"
                    },
                    {
                        "input": "./tools",
                        "glob": "generators.json",
                        "output": "."
                    },
                    {
                        "input": "./tools",
                        "glob": "executors.json",
                        "output": "."
                    }
                ]
            }
        },
        "test": {
            "executor": "@angular-devkit/build-angular:karma",
            "options": {
                "main": "tools/test.ts",
                "tsConfig": "tools/tsconfig.spec.json",
                "karmaConfig": "tools/karma.conf.js"
            }
        }
    },
    "tags": ["type:tools", "scope:tools"]
}
