{
    "extends": "./tsconfig.json",
    "compilerOptions": {
        "outDir": "../../dist/out-tsc",
        "target": "es2023",
        "declaration": true,
        "declarationMap": true,
        "types": [],
        "lib": ["dom", "es2023"],
        "useDefineForClassFields": false,
        "moduleResolution": "node",
        "skipLibCheck": true,
        "module": "preserve",
        "baseUrl": ".",
        "paths": {
            "@fundamental-ngx/ui5-webcomponents-base": ["../../dist/libs/ui5-webcomponents-base"],
            "@fundamental-ngx/ui5-webcomponents-base/theming": ["../../dist/libs/ui5-webcomponents-base/theming"],
            "@fundamental-ngx/ui5-webcomponents-base/types": ["../../dist/libs/ui5-webcomponents-base/types"],
            "@fundamental-ngx/ui5-webcomponents-base/*": ["../../dist/libs/ui5-webcomponents-base/*"],
            "@fundamental-ngx/ui5-webcomponents": ["../../dist/libs/ui5-webcomponents"],
            "@fundamental-ngx/ui5-webcomponents/theming": ["../../dist/libs/ui5-webcomponents/theming"],
            "@fundamental-ngx/ui5-webcomponents/utils": ["../../dist/libs/ui5-webcomponents/utils"],
            "@fundamental-ngx/ui5-webcomponents/*": ["../../dist/libs/ui5-webcomponents/*"]
        }
    },
    "angularCompilerOptions": {
        "skipTemplateCodegen": true,
        "strictMetadataEmit": true,
        "enableResourceInlining": true
    },
    "files": ["./index.ts"],
    "exclude": ["test.ts", "**/*.spec.ts"],
    "include": ["**/*.ts"]
}
