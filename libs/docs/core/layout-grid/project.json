{
    "name": "docs-core-layout-grid",
    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/docs/core/layout-grid",
    "projectType": "library",
    "prefix": "fundamental-ngx",
    "targets": {
        "e2e": {
            "executor": "@fundamental-ngx/nx-plugin:e2e-test",
            "options": {
                "e2eFiles": ["libs/docs/core/layout-grid/e2e/**/*.e2e-spec.ts"],
                "devServerTarget": "docs:serve:e2e"
            },
            "outputs": ["{workspaceRoot}/allure-results/docs-core-layout-grid"]
        },
        "extract-compodoc": {
            "executor": "@fundamental-ngx/nx-plugin:compodoc",
            "options": {
                "outputPath": "libs/docs/core/layout-grid/docs.json",
                "entities": ["LayoutGridComponent", "LayoutGridColDirective", "LayoutGridRowDirective"]
            },
            "outputs": ["{options}.outputPath"],
            "dependsOn": ["core:compodoc"]
        }
    },
    "tags": ["type:lib", "scope:docs"]
}
