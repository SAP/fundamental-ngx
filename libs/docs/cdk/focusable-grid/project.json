{
    "name": "docs-fn-focusable-grid",
    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/docs/fn/focusable-grid",
    "projectType": "library",
    "prefix": "fdp-doc",
    "targets": {
        "e2e-noop": {
            "executor": "@fundamental-ngx/nx-plugin:e2e-test",
            "options": {
                "e2eTsConfig": "libs/docs/fn/focusable-grid/e2e/tsconfig.json",
                "baseUrl": "http://localhost:4200",
                "configFile": "libs/docs/fn/focusable-grid/wdio.conf.js"
            },
            "outputs": ["{workspaceRoot}/allure-results/docs-fn-focusable-grid"]
        },
        "extract-compodoc": {
            "executor": "@fundamental-ngx/nx-plugin:compodoc",
            "options": {
                "outputPath": "libs/docs/cdk/focusable-grid/docs.json",
                "entities": ["FocusableGridDirective", "FocusableListDirective", "FocusableItemDirective"]
            },
            "dependsOn": ["cdk:compodoc"]
        }
    },
    "tags": ["type:lib", "scope:docs"]
}
