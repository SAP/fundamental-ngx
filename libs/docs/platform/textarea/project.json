{
    "sourceRoot": "libs/docs/platform/textarea",
    "projectType": "library",
    "prefix": "fdp-doc",
    "targets": {
        "e2e": {
            "executor": "./tools/executors/wdio:e2e-test",
            "options": {
                "e2eFiles": ["libs/docs/platform/textarea/e2e/**/*.e2e-spec.ts"],
                "devServerTarget": "docs:serve:e2e"
            },
            "outputs": ["allure-results/docs-platform-textarea"]
        },
        "build": {
            "executor": "@nrwl/angular:ng-packagr-lite",
            "outputs": ["dist/libs/docs/platform/textarea"],
            "options": {
                "project": "libs/docs/platform/textarea/ng-package.json"
            },
            "configurations": {
                "production": {
                    "tsConfig": "libs/docs/platform/textarea/tsconfig.lib.prod.json"
                },
                "development": {
                    "tsConfig": "libs/docs/platform/textarea/tsconfig.lib.json"
                }
            },
            "defaultConfiguration": "production"
        }
    },
    "tags": ["type:lib", "scope:docs"]
}
