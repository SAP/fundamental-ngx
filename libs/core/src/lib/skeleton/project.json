{
  "name": "core-skeleton",
  "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/core/src/lib/skeleton",
  "prefix": "fd",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:package",
      "outputs": [
        "{workspaceRoot}/dist/libs/core/skeleton"
      ],
      "options": {
        "project": "libs/core/src/lib/skeleton/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/core/src/lib/skeleton/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/core/src/lib/skeleton/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/core/src/lib/skeleton/**/*.ts",
          "libs/core/src/lib/skeleton/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "libs/core/src/lib/skeleton/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": [
    "type:lib",
    "scope:fd"
  ]
}
