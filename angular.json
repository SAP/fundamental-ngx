{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "",
    "projects": {
        "docs": {
            "root": "apps/docs/",
            "sourceRoot": "apps/docs/src",
            "projectType": "application",
            "prefix": "fundamental-ngx",
            "schematics": {
                "@nrwl/schematics:component": {
                    "style": "scss"
                }
            },
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "aot": true,
                        "outputPath": "dist/apps/docs",
                        "index": "apps/docs/src/index.html",
                        "main": "apps/docs/src/main.ts",
                        "polyfills": "apps/docs/src/polyfills.ts",
                        "tsConfig": "apps/docs/tsconfig.app.json",
                        "assets": [
                            "apps/docs/src/favicon.ico",
                            "apps/docs/src/assets",
                            {
                                "glob": "**/css_variables.css",
                                "input": "./node_modules/@sap-theming/theming-base-content/content/Base/baseLib/",
                                "output": "./assets/theming-base/"
                            },
                            {
                                "glob": "**/*",
                                "input": "./node_modules/fundamental-styles/dist/theming/",
                                "output": "./assets/fundamental-styles-theming/"
                            }
                        ],
                        "styles": [
                            "node_modules/fundamental-styles/dist/icon.css",
                            "apps/docs/src/fd-typedoc/assets/css/main.scss",
                            "apps/docs/src/styles.scss"
                        ],
                        "scripts": ["node_modules/marked/lib/marked.js"],
                        "showCircularDependencies": false,
                        "allowedCommonJsDependencies": [
                            "moment",
                            "highlight.js",
                            "focus-trap",
                            "@angularclass/hmr",
                            "hammerjs"
                        ]
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "apps/docs/src/environments/environment.ts",
                                    "with": "apps/docs/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                }
                            ]
                        },
                        "production-unoptimized": {
                            "tsConfig": "apps/docs/tsconfig.netlify.json",
                            "fileReplacements": [
                                {
                                    "replace": "apps/docs/src/environments/environment.ts",
                                    "with": "apps/docs/src/environments/environment.prod.ts"
                                }
                            ],
                            "aot": true,
                            "extractCss": true,
                            "sourceMap": false,
                            "namedChunks": false,
                            "vendorChunk": false,
                            "optimization": false
                        },
                        "hmr": {
                            "budgets": [
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "fileReplacements": [
                                {
                                    "replace": "apps/docs/src/environments/environment.ts",
                                    "with": "apps/docs/src/environments/environment.prod.ts"
                                }
                            ]
                        },
                        "fr": {
                            "aot": true,
                            "outputPath": "dist/libs/platform/locale/fr",
                            "i18nFile": "apps/docs/src/app/platform/locale/fr/messages.fr.xlf",
                            "i18nFormat": "xlf",
                            "i18nLocale": "fr",
                            "i18nMissingTranslation": "error"
                        },
                        "ar": {
                            "aot": true,
                            "outputPath": "dist/libs/platform/locale/ar",
                            "i18nFile": "apps/docs/src/app/platform/locale/ar/messages.ar.xlf",
                            "i18nFormat": "xlf",
                            "i18nLocale": "ar",
                            "i18nMissingTranslation": "error"
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "docs:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "docs:build:production"
                        },
                        "hmr": {
                            "browserTarget": "docs:build:hmr"
                        },
                        "fr": {
                            "browserTarget": "docs:build:fr"
                        },
                        "ar": {
                            "browserTarget": "docs:build:ar"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "docs:build"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": ["apps/docs/tsconfig.app.json", "apps/docs/tsconfig.spec.json"],
                        "exclude": ["!apps/docs/**"]
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "apps/docs/src/test.ts",
                        "tsConfig": "apps/docs/tsconfig.spec.json",
                        "karmaConfig": "apps/docs/karma.conf.js",
                        "polyfills": "apps/docs/src/polyfills.ts",
                        "styles": [
                            "node_modules/fundamental-styles/dist/fundamental-styles.css",
                            "apps/docs/src/styles.scss"
                        ],
                        "scripts": [],
                        "assets": ["apps/docs/src/favicon.ico", "apps/docs/src/assets"]
                    }
                }
            }
        },
        "docs-e2e": {
            "root": "apps/docs-e2e",
            "projectType": "application",
            "prefix": "",
            "architect": {
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "apps/docs-e2e/protractor.conf.js",
                        "devServerTarget": "docs:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "docs:serve:production"
                        }
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": "apps/docs-e2e/tsconfig.e2e.json",
                        "exclude": ["!apps/docs-e2e/**"]
                    }
                }
            }
        },
        "core": {
            "root": "libs/core",
            "sourceRoot": "libs/core/src",
            "projectType": "library",
            "prefix": "fundamental-ngx",
            "architect": {
                "build": {
                    "builder": "@nrwl/angular:package",
                    "options": {
                        "tsConfig": "libs/core/tsconfig.lib.json",
                        "project": "libs/core/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "libs/core/tsconfig.lib.prod.json"
                        }
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": ["libs/core/tsconfig.lib.json", "libs/core/tsconfig.spec.json"],
                        "exclude": ["!libs/core/**"]
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "libs/core/src/test.ts",
                        "tsConfig": "libs/core/tsconfig.spec.json",
                        "karmaConfig": "libs/core/karma.conf.js"
                    }
                }
            },
            "schematics": {
                "@nrwl/schematics:component": {
                    "styleext": "scss"
                }
            }
        },
        "platform": {
            "root": "libs/platform",
            "sourceRoot": "libs/platform/src",
            "projectType": "library",
            "prefix": "fdp",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-ng-packagr:build",
                    "options": {
                        "tsConfig": "libs/platform/tsconfig.lib.json",
                        "project": "libs/platform/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "libs/platform/tsconfig.lib.prod.json"
                        }
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": ["libs/platform/tsconfig.lib.json", "libs/platform/tsconfig.spec.json"],
                        "exclude": ["!libs/platform/**"]
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "libs/platform/src/test.ts",
                        "tsConfig": "libs/platform/tsconfig.spec.json",
                        "karmaConfig": "libs/platform/karma.conf.js"
                    }
                }
            },
            "schematics": {
                "@nrwl/schematics:component": {
                    "styleext": "scss"
                }
            }
        },
        "store": {
            "projectType": "library",
            "root": "libs/store",
            "sourceRoot": "libs/store/src",
            "prefix": "fundamental-ngx",
            "architect": {
              "build": {
                "builder": "@nrwl/angular:package",
                "options": {
                  "tsConfig": "libs/store/tsconfig.lib.json",
                  "project": "libs/store/ng-package.json"
                },
                "configurations": {
                  "production": {
                    "tsConfig": "libs/store/tsconfig.lib.prod.json"
                  }
                }
              },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": ["libs/store/tsconfig.lib.json", "libs/store/tsconfig.spec.json"],
                        "exclude": ["**/node_modules/**", "!libs/store/**/*"]
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "libs/store/src/test.ts",
                        "tsConfig": "libs/store/tsconfig.spec.json",
                        "karmaConfig": "libs/platform/karma.conf.js"
                    }
                }
            },
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            }
        },
        "store-app": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "apps/store-app",
            "sourceRoot": "apps/store-app/src",
            "prefix": "fundamental-ngx",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/apps/store-app",
                        "index": "apps/store-app/src/index.html",
                        "main": "apps/store-app/src/main.ts",
                        "polyfills": "apps/store-app/src/polyfills.ts",
                        "tsConfig": "apps/store-app/tsconfig.app.json",
                        "aot": true,
                        "assets": ["apps/store-app/src/favicon.ico", "apps/store-app/src/assets"],
                        "styles": ["apps/store-app/src/styles.scss"],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "apps/store-app/src/environments/environment.ts",
                                    "with": "apps/store-app/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "store-app:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "store-app:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "store-app:build"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "apps/store-app/tsconfig.app.json",
                            "apps/store-app/tsconfig.spec.json",
                            "apps/store-app/tsconfig.editor.json"
                        ],
                        "exclude": ["**/node_modules/**", "!apps/store-app/**/*"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "apps/store-app/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "store-app-e2e": {
            "root": "apps/store-app-e2e",
            "sourceRoot": "apps/store-app-e2e/src",
            "projectType": "application",
            "architect": {
                "e2e": {
                    "builder": "@nrwl/cypress:cypress",
                    "options": {
                        "cypressConfig": "apps/store-app-e2e/cypress.json",
                        "tsConfig": "apps/store-app-e2e/tsconfig.e2e.json",
                        "devServerTarget": "store-app:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "store-app:serve:production"
                        }
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": ["apps/store-app-e2e/tsconfig.e2e.json"],
                        "exclude": ["**/node_modules/**", "!apps/store-app-e2e/**/*"]
                    }
                }
            }
        }
    },
    "defaultProject": "docs",
    "schematics": {
        "@schematics/angular:component": {
            "prefix": "fd",
            "style": "scss"
        },
        "@schematics/angular:directive": {
            "prefix": "fd"
        },
        "@nrwl/angular:application": {
            "unitTestRunner": "jest",
            "e2eTestRunner": "cypress"
        },
        "@nrwl/angular:library": {
            "unitTestRunner": "jest"
        }
    },
    "cli": {
        "defaultCollection": "@nrwl/angular",
        "analytics": false
    }
}
