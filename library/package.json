{
    "name": "fundamental-ngx",
    "version": "0.8.0-rc.9",
    "schematics": "./schematics/collection.json",
    "description": "SAP Fiori Fundamentals, implemented in Angular",
    "license": "Apache-2.0",
    "homepage": "https://sap.github.io/fundamental-ngx/home",
    "repository": {
        "type": "git",
        "url": "https://github.com/SAP/fundamental-ngx"
    },
    "scripts": {
        "std-version": "standard-version --infile ../CHANGELOG.md -m \"chore(release): version %s build ${TRAVIS_BUILD_NUMBER} [ci skip]\"",
        "build": "../node_modules/.bin/tsc -p tsconfig.schematics.json",
        "copy:schemas": "rsync -R -r --include=schema.json schematics ../dist/fundamental-ngx/",
        "copy:collection": "cp schematics/collection.json ../dist/fundamental-ngx/schematics/collection.json",
        "postbuild": "npm run copy:schemas && npm run copy:collection"
    },
    "peerDependencies": {
        "@angular/common": "^7.2.7",
        "@angular/core": "^7.2.7",
        "@angular/forms": "^7.2.7",
        "@angular/animations": "^7.2.7",
        "fundamental-styles": "^0.1.0",
        "rxjs": "^6.4.0"
    },
    "devDependencies": {
        "standard-version": "4.4.0",
        "@schematics/angular": "^7.3.9",
        "chalk": "^2.4.2"
    },
    "dependencies": {
        "focus-trap": "^5.0.0",
        "popper.js": "^1.14.7",
        "fundamental-styles": "^0.1.0"
    }
}
